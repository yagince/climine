<% issue = res.issue %>
<<<%= issue.tracker.name %>>> [<%= issue.status.name %>] #<%= issue.id %> <%= issue.subject %>
----------------------------------------------------------------------------------------
[PROJECT ] <%= issue.project.name %>
[created ] <%= Time.parse(issue.created_on).localtime.strftime("%Y-%m-%d %H:%M:%S") %>
[Author  ] <%= issue.author.name %>
[Assigned] <%= issue.assigned_to ? issue.assigned_to.name : "Unassigned" %>

<%= issue.description %>
----------------------------------------------------------------------------------------
<%- if issue.journals -%>
< NOTES >
<%- issue.journals.each_with_index{|journal, i| -%>
  <%- if journal.notes -%>
[ #<%= i+1 %> <%= journal.user.name %> ]
<%= journal.notes %>
  <%- end -%>
<%- } -%>
<%- end -%>
